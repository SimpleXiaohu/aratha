{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    "@babel/preset-flow"
  ],
  "plugins": [
    "@babel/plugin-transform-logical-assignment-operators",
    "./plugins/logicalExp.js"
  ],
  "parserOpts": {
    "sourceType": "unambiguous",
    "plugins": [
      "jsx",
      "flow"
    ]
  }
}